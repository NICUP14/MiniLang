{
  "name": "minilang-highlighter",
  "displayName": "MiniLang Highlighter",
  "description": "",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.77.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [
      {
        "id": "minilang",
        "aliases": [
          "MiniLang",
          "minilang"
        ],
        "extensions": [
          ".ml"
        ],
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "minilang",
        "scopeName": "source.ml",
        "path": "./syntaxes/minilang.tmLanguage.json"
      }
    ],
    "tokenColors": [
      {
        "scope": "support.function.minilang",
        "settings": {
          "foreground": "#70c6ae"
        }
      },
      {
        "scope": "storage.type.minilang",
        "settings": {
          "foreground": "#b885bc"
        }
      }
    ]
  }
}